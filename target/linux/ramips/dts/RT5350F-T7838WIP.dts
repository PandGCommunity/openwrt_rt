/dts-v1/;

#include "rt5350.dtsi"

/ {
    compatible = "vstarcam,rt5350f-t7838wip", "ralink,rt5350-soc";
    model = "Vstarcam T7838WIP";

    aliases {
        serial0 = &uartlite;
        serial1 = &uart;
    };
};


&spi0 {
    status = "okay";

    m25p80@0 {
        #address-cells = <1>;
        #size-cells = <1>;
        compatible = "jedec,spi-nor";
        reg = <0>;
        spi-max-frequency = <10000000>;

        partition@0 {
            label = "u-boot";
            reg = <0x0 0x30000>;
            read-only;
        };

        partition@30000 {
            label = "u-boot-env";
            reg = <0x30000 0x10000>;
            read-only;
        };

        factory: partition@40000 {
            label = "factory";
            reg = <0x40000 0x10000>;
            read-only;
        };

        partition@50000 {
            label = "firmware";
            reg = <0x50000 0x7b0000>;
        };
    };
};

&gpio1 {
    status = "okay";
};

&pinctrl {
    state_default: pinctrl0 {
        gpio {
            ralink,group = "led";
            // ralink,group = "i2c", "led";
            // ralink,group = "jtag", "led";
            ralink,function = "gpio";
        };
        i2s {
            ralink,group = "uartf";
            ralink,function = "pcm i2s";
        };
    };
};

&ethernet {
    mtd-mac-address = <&factory 0x4>;
};

&esw {
    mediatek,portmap = <0x2f>;
    mediatek,led_polarity = <0x17>;
};

&wmac {
    ralink,mtd-eeprom = <&factory 0>;
    ralink,led-polarity = <1>;
};

&ehci {
    status = "okay";
};

&ohci {
    status = "okay";
};

&i2c {
    status = "okay";

    htu: htu21@40 {
        compatible = "htu21";
        reg = <0x40>;
    };

    wm8988: wm8988@1b {
        compatible = "wm8988";
        reg = <0x1b>;
    };
};

&i2s {
    status = "okay";
};

&gdma {
    status = "okay";
};

&uart {
    status = "okay";
};

/*
 * Audio:
 * Audio Multiplexer
 * I2C mit Wolfson WM8988
 */
/ {
    sound {
        compatible = "ralink,rt5350f-wm8988-machine";
        ralink,platform = <&i2s>;
        ralink,audio-codec = <&wm8988>;
        audio-routing =
            "Headphone", "LOUT1",
            "Headphone", "ROUT1",
            "Ext Spk", "LOUT2",
            "Ext Spk", "ROUT2",
            "LINPUT1", "Mic",
            "RINPUT1", "Mic";
    };
};
